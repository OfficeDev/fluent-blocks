{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "Text",
  "description": "A piece of text content.",
  "properties": {
    "text": {
      "type": "string"
    },
    "variant": {
      "title": "Text variant",
      "enum": ["normal", "emphasized", "strong", "highlighted"]
    }
  },
  "required": ["text"],
  "additionalProperties": false
}